<?xml version = '1.0' encoding = 'windows-1252'?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">
  <servlet>
    <servlet-name>Filter</servlet-name>
    <servlet-class>project1.Filter</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Home</servlet-name>
    <servlet-class>project1.Home</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>InsertSimilar</servlet-name>
    <servlet-class>project1.InsertSimilar</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Insert</servlet-name>
    <servlet-class>project1.Insert</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UpdateSimilar</servlet-name>
    <servlet-class>project1.UpdateSimilar</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Update</servlet-name>
    <servlet-class>project1.Update</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>UpdateForm</servlet-name>
    <servlet-class>project1.UpdateForm</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>deleterow</servlet-name>
    <servlet-class>project1.deleterow</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Home</servlet-name>
    <url-pattern>/home</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>InsertSimilar</servlet-name>
    <url-pattern>insertsimilar</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Insert</servlet-name>
    <url-pattern>insert</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UpdateSimilar</servlet-name>
    <url-pattern>/updatesimilar</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Update</servlet-name>
    <url-pattern>/update</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>UpdateForm</servlet-name>
    <url-pattern>/updateform</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>deleterow</servlet-name>
    <url-pattern>/deleterow</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Filter</servlet-name>
    <url-pattern>/filter</url-pattern>
  </servlet-mapping>
</web-app>
